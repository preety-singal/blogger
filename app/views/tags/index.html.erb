<h1>All Tags</h1>
<ul id ="tags">
<% @tags.each do |tag| %>
	<li>
	<%= link_to tag.name, tag_path(:id=>tag.id,:tag=>1) %>
	</li>
	<% if user_signed_in? %>
	<%= link_to "edit", edit_tag_path(tag) %>
	<%= link_to "delete", tag_path(tag), method: :delete,data: {confirm: "Really delete the tag?"}%>
	<% end %>
 <% end %>


 </ul>